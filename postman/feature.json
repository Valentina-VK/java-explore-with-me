		{
			"name": "Отмена подписки на пользователя",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const main = async () => {\r",
							"    const api = new API(pm);\r",
							"    const rnd = new RandomUtils();\r",
							"\r",
							"    try {\r",
							"        const user = await api.addUser(rnd.getUser());\r",
							"        pm.collectionVariables.set(\"uid\", user.id)\r",
							"        const user1 = await api.addUser(rnd.getUser());\r",
							"        pm.collectionVariables.set(\"uid1\", user1.id)\r",
							"        await api.setPermissionTrue(user1.id);\r",
							"        const subsc = await api.activateSubscripton(user.id, user1.id);\r",
							"        pm.collectionVariables.set(\"countSubscribersBefore\", subsc.countSubscribers);\r",
							"    } catch(err) {\r",
